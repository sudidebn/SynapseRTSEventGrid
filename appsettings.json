{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "mongoDb-connection": "mongodb+srv://<user id>:<passsword>@<cluster_name.uniqueid>mongodb.net/retail?retryWrites=true&w=majority",
  "topic-endpoint": "https://<eventtopic>.<location>.eventgrid.azure.net/api/events",
  "topic-key": "event grid topic access key",
  "mongodb-database": "mongodb database name",
  "mongodb-collection": "mongodb collection name",
  "dataLakeGen2-accountName": "datalake gen2 account name",
  "dataLakeGen2-accountKey": "datalake gen2 key",
  "fileSystemName": "data lake gen 2 container name",
  "dataLakeGen2Uri": "https://<ADLS gen2 uri>.dfs.core.windows.net",
  "container": "directory within ADLS Gen2 container",
  "fileType": "parquet"
}
